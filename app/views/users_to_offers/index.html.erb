<p id="notice"><%= notice %></p>

<h1>Users To Offers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Offer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_to_offers.each do |users_to_offer| %>
      <tr>
        <td><%= users_to_offer.user_id %></td>
        <td><%= users_to_offer.offer_id %></td>
        <td><%= link_to 'Show', users_to_offer %></td>
        <td><%= link_to 'Edit', edit_users_to_offer_path(users_to_offer) %></td>
        <td><%= link_to 'Destroy', users_to_offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

